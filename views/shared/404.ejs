<main class="status-page">
  <h1 class="status-title status-title--xl status-title--gradient">404</h1>
  <p class="status-text">
    <%= typeof message !== 'undefined' && message ? message : "The page you were looking for doesn't exist or may have been moved." %>
  </p>
  <a href="/" class="btn btn-info link-no-underline">Return Home</a>
</main>
