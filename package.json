{
  "name": "@gabriel20xx/nude-shared",
  "version": "1.1.2",
  "description": "Shared utilities and client assets for NudeFlow and NudeForge",
  "type": "module",
  "main": "./server/logger/serverLogger.js",
  "module": "./server/logger/serverLogger.js",
  "scripts": {
    "test": "node scripts/run-all-tests.mjs",
    "test:watch": "vitest --config vitest.config.mjs --watch",
    "lint": "eslint . --ext .js,.mjs --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "test:quick": "vitest run --include 'test/smoke/**/*.test.mjs' --include 'test/**/authHealth.test.mjs' --include 'test/**/forgeAuthHealth.test.mjs' --include 'test/**/flowAuthHealth.test.mjs'",
    "test:timing": "node scripts/report-test-timings.mjs",
    "test:skips": "node scripts/list-skipped-tests.mjs",
  "lint:all": "node scripts/lint-all.mjs",
  "lint:all:json": "node scripts/lint-all-json.mjs",
  "test:session": "vitest run --include 'test/**/sessionCookieConsistency.test.mjs'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gabriel20xx/NudeShared.git"
  },
  "exports": {
    ".": {
      "import": "./server/logger/serverLogger.js"
    },
    "./server/index.js": "./server/index.js",
    "./server/logger/serverLogger.js": "./server/logger/serverLogger.js",
    "./clientLogger.js": "./clientLogger.js",
    "./theme.css": "./theme.css"
  },
  "files": [
    "server/**",
    "clientLogger.js",
    "theme.css",
    "README.md"
  ],
  "keywords": [
    "nude",
    "shared",
    "logger",
    "utilities"
  ],
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@gabriel20xx/nude-shared": "file:",
    "archiver": "^7.0.1",
    "better-sqlite3": "^9.6.0",
    "express": "^4.21.2",
    "express-session": "^1.17.3",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "ejs": "^3.1.10",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "jsdom": "^24.0.0",
    "supertest": "^7.1.1",
    "vitest": "^1.6.1"
  }
}
